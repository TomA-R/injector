{
  "name": "bigcommerce/injector",
  "type": "library",
  "description": "Container interop Dependency Injector.",
  "keywords": [
    "di",
    "pimple",
    "injector"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Benson",
      "email": "adam.benson@bigcommerce.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6",
    "pimple/pimple": "*",
    "ocramius/proxy-manager": "^1.0 || ^2.0",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0 || ^5.6"
  },
  "autoload": {
    "psr-4": {
      "Bigcommerce\\Injector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
